<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DesignCustomFont">
        <attr name="BaturaFontType" format="string"/>
    </declare-styleable>

        <declare-styleable name="CustomEditTextView">
            <attr name="drawableLeftCompat" format="reference"/>
            <attr name="drawableRightCompat" format="reference"/>
            <attr name="drawableTopCompat" format="reference"/>
            <attr name="drawableBottomCompat" format="reference"/>
            <attr name="editable" format="boolean"/>
        </declare-styleable>


    <declare-styleable name="BaturaImageText">
        <attr name="titleText" format="string" />
        <attr name="imageSrc" format="integer" />
        <attr name="normalColor" format="color" />
        <attr name="selectedColor" format="color" />
        <attr name="titleTextSize" format="dimension"/>
    </declare-styleable>

</resources>